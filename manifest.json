{
    "manifest_version": 2,
    "name": "Privacy Setup",
    "version": "0.2.0",
    "description": "Prenez en mains votre vie priv√©.",
    "icons": {
        "48": "img/PSV_48.png",
        "96": "img/PSV_96.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "browser_style": false,
        "default_icon": {
            "16": "img/PSV_16.png",
            "32": "img/PSV_32.png"
        },
        "default_title": "Privacy Setup",
        "default_popup": "popup/window.html"
    },
    "content_scripts": [
        {
            "all_frames": true,
            "run_at": "document_start",
            "matches": [
                "*://*.youtube.com/*",
                "*://*.youtu.be/*",
                "*://*.youtube.fr/*"
            ],
            "js": [
                "redirect_piped.js"
            ]
        },
        {
            "all_frames": true,
            "run_at": "document_start",
            "matches": [
                "*://www.twitter.com/*",
                "*://twitter.com/*",
                "*://*.t.co/*"
            ],
            "js": [
                "redirect_nitter.js"
            ]
        }
    ]
}